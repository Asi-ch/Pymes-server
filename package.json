  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "tsc": "tsc",
    "build": "npm-run-all clean tsc",
    "dev:start": "npm-run-all build start",
    "dev": "nodemon --watch src -e ts,ejs --exec npm run dev:start",
    "start": "node .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate": "npx sequelize db:migrate",
    "migrate:create": "npx sequelize migration:generate --name",
    "migrate:undo": "npx sequelize db:migrate:undo --name",
    "migrate:heroku": "heroku run sequelize db:migrate --remote heroku",
    "seed": "npx sequelize db:seed:all"
  },
